<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <padding
        android:bottom="@dimen/five_dp"
        android:left="@dimen/five_dp"
        android:right="@dimen/five_dp"
        android:top="@dimen/five_dp" />
    <item
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/greennn"/>
            <stroke android:color="@color/greennn"/>
        </shape>
    </item>

    <item
        android:top="5dp"  android:right="1dp" android:left="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>